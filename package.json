{
  "name": "@handsfree/accessangel",
  "version": "1.0.3",
  "description": "AccessAngel is a toolbar that provides a range of features to make any website accessible.",
  "author": "Elliott Judd <elliott.judd@hands-free.co.uk>",
  "license": "AGPL-3.0-or-later",
  "main": "public/dist/accessangel/accessangel.bundle.js",
  "files": ["public"],
  "scripts": {
    "prepare": "rimraf \"./public/dist/accessangel/*\" \"./node_modules/.cache/*\" && webpack --env.mode=\"production\" --env.npm=\"enabled\"",
    "extension": "webpack --env.mode=\"production\" --env.extension=\"enabled\"",
    "build": "webpack --env.mode=\"production\"",
    "b": "webpack --env.mode=\"development\"",
    "b:v": "webpack --env.mode=\"development\" --env.verbose=\"enabled\"",
    "server": "webpack-dashboard -- webpack-dev-server --env.mode=\"development\" --env.server=\"enabled\" --env.dash=\"enabled\"",
    "s": "webpack-dashboard -- webpack-dev-server --env.mode=\"development\" --env.server=\"enabled\" --env.dash=\"enabled\"",
    "bp": "webpack --env.mode=\"production\"",
    "bp:v": "webpack --env.mode=\"production\" --env.verbose=\"enabled\"",
    "clean": "rimraf \"./public/dist/accessangel/*\" \"./node_modules/.cache/*\"",
    "cypress": "cypress open",
    "cypress:run-all": "cypress run",
    "ava": "ava",
    "test": "ava && cypress run",
    "serve": "serve ./public/",
    "now-build": "yarn bp",
    "check": "gts check",
    "fix": "gts fix"
  },
  "browserslist": [
    "defaults",
    "last 2 years"
  ],
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/node": "13.11.1",
    "@types/webpack": "4.41.21",
    "@types/webpack-env": "1.15.2",
    "autoprefixer": "9.8.5",
    "ava": "3.10.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "browser-sync": "2.26.10",
    "browser-sync-webpack-plugin": "2.2.2",
    "cache-loader": "4.1.0",
    "css-hot-loader": "1.4.4",
    "css-loader": "3.6.0",
    "cypress": "4.11.0",
    "eslint-plugin-cypress": "2.11.1",
    "file-loader": "6.0.0",
    "gts": "2.0.2",
    "hard-source-webpack-plugin": "0.13.1",
    "json5-loader": "4.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "offline-plugin": "5.0.7",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-custom-media": "7.0.8",
    "postcss-easy-import": "3.0.0",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.3",
    "postcss-nested-ancestors": "2.0.0",
    "postcss-nested-props": "2.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-sorting": "5.0.1",
    "resource-hints-webpack-plugin": "0.0.2",
    "rimraf": "3.0.2",
    "rucksack-css": "1.0.2",
    "source-map-loader": "1.0.1",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0",
    "terser-webpack-plugin": "3.0.7",
    "thread-loader": "2.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "url-loader": "^4.1.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dashboard": "3.2.0",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@simonwep/pickr": "1.7.2",
    "@typescript-eslint/parser": "3.10.1",
    "@webcomponents/custom-elements": "1.4.2",
    "bignumber.js": "9.0.0",
    "core-js": "^3.6.5",
    "dompurify": "^2.2.2",
    "hotkeys-js": "3.8.1",
    "hyperapp": "2.0.4",
    "is-mobile": "2.2.2",
    "iso-639-1": "2.1.3",
    "normalize.css": "8.0.1",
    "regenerator-runtime": "^0.13.7",
    "serve": "11.3.2",
    "tippy.js": "6.2.5",
    "uuid": "8.2.0"
  }
}
